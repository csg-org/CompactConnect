{
  "additionalProperties": false,
  "properties": {
    "username": {
      "description": "Provider's email address (username)",
      "format": "email",
      "maxLength": 100,
      "minLength": 5,
      "type": "string"
    },
    "password": {
      "description": "Provider's current password",
      "maxLength": 256,
      "minLength": 12,
      "type": "string"
    },
    "compact": {
      "description": "Compact abbreviation",
      "enum": [
        "aslp",
        "octp",
        "coun"
      ],
      "type": "string"
    },
    "jurisdiction": {
      "description": "Two-letter jurisdiction code",
      "enum": [
        "al",
        "ak",
        "az",
        "ar",
        "ca",
        "co",
        "ct",
        "de",
        "dc",
        "fl",
        "ga",
        "hi",
        "id",
        "il",
        "in",
        "ia",
        "ks",
        "ky",
        "la",
        "me",
        "md",
        "ma",
        "mi",
        "mn",
        "ms",
        "mo",
        "mt",
        "ne",
        "nv",
        "nh",
        "nj",
        "nm",
        "ny",
        "nc",
        "nd",
        "oh",
        "ok",
        "or",
        "pa",
        "pr",
        "ri",
        "sc",
        "sd",
        "tn",
        "tx",
        "ut",
        "vt",
        "va",
        "vi",
        "wa",
        "wv",
        "wi",
        "wy"
      ],
      "type": "string"
    },
    "givenName": {
      "description": "Provider's given name",
      "maxLength": 200,
      "minLength": 1,
      "type": "string"
    },
    "familyName": {
      "description": "Provider's family name",
      "maxLength": 200,
      "minLength": 1,
      "type": "string"
    },
    "dob": {
      "description": "Date of birth in YYYY-MM-DD format",
      "format": "date",
      "pattern": "^[12]{1}[0-9]{3}-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])$",
      "type": "string"
    },
    "partialSocial": {
      "description": "Last 4 digits of SSN",
      "pattern": "^[0-9]{4}$",
      "type": "string"
    },
    "licenseType": {
      "description": "Type of license",
      "enum": [
        "audiologist",
        "speech-language pathologist",
        "occupational therapist",
        "occupational therapy assistant",
        "licensed professional counselor"
      ],
      "type": "string"
    },
    "recaptchaToken": {
      "description": "ReCAPTCHA token for verification",
      "minLength": 1,
      "type": "string"
    }
  },
  "required": [
    "username",
    "password",
    "compact",
    "jurisdiction",
    "givenName",
    "familyName",
    "dob",
    "partialSocial",
    "licenseType",
    "recaptchaToken"
  ],
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#"
}
