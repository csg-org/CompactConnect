{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "dateOfUpdate": {
      "description": "The date the document was last updated",
      "format": "date",
      "pattern": "^[12]{1}[0-9]{3}-[01]{1}[0-9]{1}-[0-3]{1}[0-9]{1}$",
      "type": "string"
    },
    "dateOfUpload": {
      "description": "The date the document was uploaded",
      "format": "date",
      "pattern": "^[12]{1}[0-9]{3}-[01]{1}[0-9]{1}-[0-3]{1}[0-9]{1}$",
      "type": "string"
    },
    "documentPostUrl": {
      "description": "The url to post the document to",
      "type": "string"
    },
    "fileName": {
      "description": "The name of the file being uploaded",
      "type": "string"
    },
    "status": {
      "description": "The status of the document",
      "type": "string"
    },
    "type": {
      "description": "The type of document being uploaded",
      "enum": [
        "militaryAffiliation"
      ],
      "type": "string"
    }
  },
  "required": [
    "type",
    "documentPostUrl",
    "fileName",
    "status",
    "dateOfUpload",
    "dateOfUpdate"
  ],
  "type": "object"
}
