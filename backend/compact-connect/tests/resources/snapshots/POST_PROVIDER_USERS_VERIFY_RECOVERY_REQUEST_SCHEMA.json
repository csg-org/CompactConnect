{
  "additionalProperties": false,
  "properties": {
    "compact": {
      "description": "Compact abbreviation",
      "enum": [
        "aslp",
        "octp",
        "coun"
      ],
      "type": "string"
    },
    "providerId": {
      "description": "Provider UUID",
      "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab]{1}[0-9a-f]{3}-[0-9a-f]{12}",
      "type": "string"
    },
    "recoveryToken": {
      "description": "Recovery token from the email link",
      "maxLength": 256,
      "minLength": 1,
      "type": "string"
    },
    "recaptchaToken": {
      "description": "ReCAPTCHA token for verification",
      "minLength": 1,
      "type": "string"
    }
  },
  "required": [
    "compact",
    "providerId",
    "recoveryToken",
    "recaptchaToken"
  ],
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#"
}
